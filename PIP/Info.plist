	<array>
		<dict>
			<key>CFBundleTypeExtensions</key>
			<array>
				<string>sh</string>
				<string>bash</string>
				<string>zsh</string>
				<string>py</string>
				<string>rb</string>
				<string>js</string>
				<string>ts</string>
				<string>swift</string>
				<string>c</string>
				<string>cpp</string>
				<string>h</string>
				<string>hpp</string>
				<string>txt</string>
				<string>md</string>
				<string>json</string>
				<string>xml</string>
				<string>yaml</string>
				<string>yml</string>
			</array>
			<key>CFBundleTypeName</key>
			<string>Text Document</string>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>LSHandlerRank</key>
			<string>Alternate</string>
		</dict>
	</array>
